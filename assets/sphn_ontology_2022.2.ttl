@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix atc: <https://www.whocc.no/atc_ddd_index/?code=> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix chop: <https://biomedit.ch/rdf/sphn-resource/chop/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sphn: <https://biomedit.ch/rdf/sphn-ontology/sphn#> .
@prefix ucum: <https://biomedit.ch/rdf/sphn-resource/ucum/> .
@prefix loinc: <https://loinc.org/rdf/> .
@prefix snomed: <http://snomed.info/id/> .
@prefix sphn-atc: <https://biomedit.ch/rdf/sphn-resource/atc/> .
@prefix icd-10-gm: <https://biomedit.ch/rdf/sphn-resource/icd-10-gm/> .
@prefix sphn-loinc: <https://biomedit.ch/rdf/sphn-resource/loinc/> .
@base <https://biomedit.ch/rdf/sphn-ontology/sphn> .

<https://biomedit.ch/rdf/sphn-ontology/sphn> rdf:type owl:Ontology ;
                                              owl:versionIRI <https://biomedit.ch/rdf/sphn-ontology/sphn/2022/2> ;
                                              owl:imports <http://snomed.info/sct/900000000000207008/version/20220207> ,
                                                          <https://biomedit.ch/rdf/sphn-resource/atc/2022/1> ,
                                                          <https://biomedit.ch/rdf/sphn-resource/chop/2022/4> ,
                                                          <https://biomedit.ch/rdf/sphn-resource/icd-10-gm/2022/2> ,
                                                          <https://biomedit.ch/rdf/sphn-resource/loinc/2.72/1> ,
                                                          <https://biomedit.ch/rdf/sphn-resource/ucum/2021/1> ;
                                              dc:description "The RDF schema describing concepts defined in the official SPHN dataset"@en ;
                                              dc:title "The SPHN RDF Schema"@en ;
                                              dct:license <https://creativecommons.org/licenses/by-nc-sa/4.0/> ;
                                              owl:priorVersion <https://biomedit.ch/rdf/sphn-ontology/sphn/2022/1> .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#maxCardinality
owl:maxCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#replaces
sphn:replaces rdf:type owl:AnnotationProperty ;
              rdfs:comment "Property that links the new version of an SPHN class replacing an SPHN old class" .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gDay
xsd:gDay rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonth
xsd:gMonth rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#time
xsd:time rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://biomedit.ch/rdf/sphn-ontology/sphn#SPHNAttributeObject
sphn:SPHNAttributeObject rdf:type owl:ObjectProperty ;
                         rdfs:comment "SPHN object attribute defined by the dataset" ;
                         rdfs:label "SPHN attribute object" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasActiveIngredient
sphn:hasActiveIngredient rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                         rdfs:domain sphn:Drug ;
                         rdfs:range sphn:Substance ;
                         rdfs:comment "pharmaceutically active component of a drug" ;
                         rdfs:label "has active ingredient" ;
                         sphn:replaces sphn:hasDrugActiveIngredientSubstance .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasAdministrableDoseForm
sphn:hasAdministrableDoseForm rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                              rdfs:domain [ rdf:type owl:Class ;
                                            owl:unionOf ( sphn:DrugAdministrationEvent
                                                          sphn:DrugPrescription
                                                        )
                                          ] ;
                              rdfs:range sphn:PharmaceuticalDoseForm ;
                              rdfs:comment "dose form of the drug for administration to a patient, after any necessary transformation has been carried out, e.g. solution" ;
                              rdfs:label "has administrable dose form" ;
                              sphn:replaces sphn:hasGalenicCode .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasAdministrationRouteCode
sphn:hasAdministrationRouteCode rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf sphn:hasCode ;
                                rdfs:domain [ rdf:type owl:Class ;
                                              owl:unionOf ( sphn:DrugAdministrationEvent
                                                            sphn:DrugPrescription
                                                          )
                                            ] ;
                                rdfs:range [ rdf:type owl:Class ;
                                             owl:unionOf ( sphn:Code
                                                           sphn:Terminology
                                                         )
                                           ] ;
                                rdfs:comment "code, name, coding system and version describing the route of administration" ;
                                rdfs:label "has administration route code" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasAdministrativeCase
sphn:hasAdministrativeCase rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( sphn:AdverseEvent
                                                       sphn:AllergyEpisode
                                                       sphn:Biobanksample
                                                       sphn:Biosample
                                                       sphn:BloodPressure
                                                       sphn:BodyHeight
                                                       sphn:BodyTemperature
                                                       sphn:BodyWeight
                                                       sphn:CircumferenceMeasure
                                                       sphn:Diagnosis
                                                       sphn:DrugAdministrationEvent
                                                       sphn:DrugPrescription
                                                       sphn:GestationalAgeAtBirth
                                                       sphn:HealthcareEncounter
                                                       sphn:HeartRate
                                                       sphn:InhaledOxygenConcentration
                                                       sphn:LabResult
                                                       sphn:OncologyTreatmentAssessment
                                                       sphn:OxygenSaturation
                                                       sphn:ProblemCondition
                                                       sphn:Procedure
                                                       sphn:RespiratoryRate
                                                       sphn:SimpleScore
                                                       sphn:TNMClassification
                                                       sphn:TumorGrade
                                                       sphn:TumorSpecimen
                                                       sphn:TumorStage
                                                     )
                                       ] ;
                           rdfs:range sphn:AdministrativeCase ;
                           rdfs:comment "link to an interaction between an individual and healthcare provider(s) via an administrative case identifier for the purpose of providing healthcare service(s) or assessing the health status of an individual" ;
                           rdfs:label "has administrative case" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasBiosample
sphn:hasBiosample rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( sphn:Biobanksample
                                              sphn:LabResult
                                              sphn:TumorSpecimen
                                            )
                              ] ;
                  rdfs:range sphn:Biosample ;
                  rdfs:comment "any material sample taken from a biological entity for testing, diagnostic, propagation, treatment or research purposes" ;
                  rdfs:label "has biosample" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasBodySite
sphn:hasBodySite rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( sphn:AllergyEpisode
                                             sphn:Biosample
                                             sphn:BloodPressure
                                             sphn:BodyTemperature
                                             sphn:CircumferenceMeasure
                                             sphn:HeartRate
                                             sphn:OncologyTreatmentAssessment
                                             sphn:OxygenSaturation
                                             sphn:Procedure
                                           )
                             ] ;
                 rdfs:range sphn:BodySite ;
                 rdfs:comment "body site where the concept was measured, performed or collected" ;
                 rdfs:label "has body site" ;
                 sphn:replaces sphn:hasBiosampleBodySite ,
                               sphn:hasCircumferenceMeasureBodySite .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasCareHandling
sphn:hasCareHandling rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                     rdfs:domain sphn:AdministrativeCase ;
                     rdfs:range sphn:CareHandling ;
                     rdfs:comment "describes the relationship between the patient and care provider institute" ;
                     rdfs:label "has care handling" ;
                     sphn:replaces sphn:hasAdministrativeCaseCareHandling .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasCertaintyCode
sphn:hasCertaintyCode rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sphn:hasCode ;
                      rdfs:domain sphn:AllergyEpisode ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( sphn:Code
                                                 sphn:Terminology
                                               )
                                 ] ;
                      rdfs:comment "code, name, coding system and version describing the degree of certainty (from a clinical point of view) that the specific substance triggered the event" ;
                      rdfs:label "has certainty code" ;
                      sphn:replaces sphn:hasAllergyEpisodeCertaintyCode .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasClass
sphn:hasClass rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf sphn:SPHNAttributeObject ;
              rdfs:domain sphn:Location ;
              rdfs:range sphn:SPHNConcept ;
              rdfs:comment "general location type; can be within a hospital (department) or outside (home, retirement home)" ;
              rdfs:label "has class" ;
              sphn:replaces sphn:hasLocationClass .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasCode
sphn:hasCode rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf sphn:SPHNAttributeObject ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( sphn:AdministrativeGender
                                         sphn:AdverseEvent
                                         sphn:BodySite
                                         sphn:CareHandling
                                         sphn:CivilStatus
                                         sphn:Consent
                                         sphn:DataDetermination
                                         sphn:DataProviderInstitute
                                         sphn:DeathStatus
                                         sphn:Diagnosis
                                         sphn:DiagnosticRadiologicExamination
                                         sphn:Drug
                                         sphn:Intent
                                         sphn:LabResult
                                         sphn:LabTest
                                         sphn:Laterality
                                         sphn:MeasurementMethod
                                         sphn:MedicalDevice
                                         sphn:OncologyTreatmentAssessment
                                         sphn:PharmaceuticalDoseForm
                                         sphn:ProblemCondition
                                         sphn:Procedure
                                         sphn:SimpleScore
                                         sphn:Substance
                                         sphn:TimePattern
                                         sphn:TumorGrade
                                         sphn:TumorStage
                                         sphn:Unit
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( sphn:Code
                                        sphn:Terminology
                                      )
                        ] ;
             rdfs:comment "code, name, coding system and version describing the concept" ;
             rdfs:label "has code" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasComparator
sphn:hasComparator rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( sphn:BirthDate
                                               sphn:Quantity
                                             )
                               ] ;
                   rdfs:range sphn:SPHNConcept ;
                   rdfs:comment "qualifier describing whether the value is the precise one or not" ;
                   rdfs:label "has comparator" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasConsequences
sphn:hasConsequences rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                     rdfs:domain sphn:AdverseEvent ;
                     rdfs:range sphn:SPHNConcept ;
                     rdfs:comment "consequence of the adverse event, with different severity" ;
                     rdfs:label "has consequences" ;
                     sphn:replaces sphn:hasAdverseEventConsequences .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasCriteria
sphn:hasCriteria rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                 rdfs:domain sphn:OncologyTreatmentAssessment ;
                 rdfs:range sphn:SPHNConcept ;
                 rdfs:comment "criteria used for the assessment" ;
                 rdfs:label "has criteria" ;
                 sphn:replaces sphn:hasOncologyTreatmentAssessmentCriteria .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasCurrentLocation
sphn:hasCurrentLocation rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf sphn:hasLocation ;
                        rdfs:domain sphn:HealthcareEncounter ;
                        rdfs:range sphn:Location ;
                        rdfs:comment "physical location where the individual received medical care" ;
                        rdfs:label "has current location" ;
                        sphn:replaces sphn:hasHealthcareEncounterCurrentLocation .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasDataDetermination
sphn:hasDataDetermination rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( sphn:InhaledOxygenConcentration
                                                      sphn:RespiratoryRate
                                                    )
                                      ] ;
                          rdfs:range sphn:DataDetermination ;
                          rdfs:comment "indicates whether the value was measured, calculated or set as a parameter of the ventilator" ;
                          rdfs:label "has data determination" ;
                          sphn:replaces sphn:hasInhaledOxygenConcentrationDataDetermination .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasDataProviderInstitute
sphn:hasDataProviderInstitute rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                              rdfs:domain [ rdf:type owl:Class ;
                                            owl:unionOf ( sphn:AdministrativeCase
                                                          sphn:AdverseEvent
                                                          sphn:Allergy
                                                          sphn:AllergyEpisode
                                                          sphn:Biobanksample
                                                          sphn:Biosample
                                                          sphn:BloodPressure
                                                          sphn:BodyHeight
                                                          sphn:BodyTemperature
                                                          sphn:BodyWeight
                                                          sphn:CircumferenceMeasure
                                                          sphn:Consent
                                                          sphn:Diagnosis
                                                          sphn:DrugAdministrationEvent
                                                          sphn:DrugPrescription
                                                          sphn:GestationalAgeAtBirth
                                                          sphn:HealthcareEncounter
                                                          sphn:HeartRate
                                                          sphn:InhaledOxygenConcentration
                                                          sphn:LabResult
                                                          sphn:Location
                                                          sphn:OncologyTreatmentAssessment
                                                          sphn:OxygenSaturation
                                                          sphn:ProblemCondition
                                                          sphn:Procedure
                                                          sphn:RespiratoryRate
                                                          sphn:SimpleScore
                                                          sphn:SubjectPseudoIdentifier
                                                          sphn:TNMClassification
                                                          sphn:TumorGrade
                                                          sphn:TumorSpecimen
                                                          sphn:TumorStage
                                                        )
                                          ] ;
                              rdfs:range sphn:DataProviderInstitute ;
                              rdfs:comment "link to the unique business identification number (UID) of the healthcare institute providing the data" ;
                              rdfs:label "has data provider institute" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasDeathDate
sphn:hasDeathDate rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                  rdfs:domain sphn:DeathStatus ;
                  rdfs:range sphn:DeathDate ;
                  rdfs:comment "datetime of death for the individual" ;
                  rdfs:label "has death date" ;
                  sphn:replaces sphn:hasDeathStatusDateTime .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasDiastolicPressure
sphn:hasDiastolicPressure rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf sphn:hasQuantity ;
                          rdfs:domain sphn:BloodPressure ;
                          rdfs:range sphn:Quantity ;
                          rdfs:comment "measured diastolic pressure and unit" ;
                          rdfs:label "has diastolic pressure" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasDischargeLocation
sphn:hasDischargeLocation rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf sphn:hasLocation ;
                          rdfs:domain sphn:AdministrativeCase ;
                          rdfs:range sphn:Location ;
                          rdfs:comment "location where the individual went to when leaving the healthcare provider institute, e.g. nursing home" ;
                          rdfs:label "has discharge location" ;
                          sphn:replaces sphn:hasAdministrativeCaseDischargeLocation .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasDrug
sphn:hasDrug rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf sphn:SPHNAttributeObject ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( sphn:DrugAdministrationEvent
                                         sphn:DrugPrescription
                                       )
                         ] ;
             rdfs:range sphn:Drug ;
             rdfs:comment "drug administered or prescribed to a patient" ;
             rdfs:label "has drug" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasDrugQuantity
sphn:hasDrugQuantity rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf sphn:hasQuantity ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( sphn:DrugAdministrationEvent
                                                 sphn:DrugPrescription
                                               )
                                 ] ;
                     rdfs:range sphn:Quantity ;
                     rdfs:comment "quantity of a drug prescribed or admnistered to an individual" ;
                     rdfs:label "has drug quantity" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasDuration
sphn:hasDuration rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sphn:hasQuantity ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( sphn:AllergyEpisode
                                             sphn:DrugAdministrationEvent
                                           )
                             ] ;
                 rdfs:range sphn:Quantity ;
                 rdfs:comment "time interval during which a substance was administered; it is only used for continuously administered substances" ;
                 rdfs:label "has duration" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasExposureDuration
sphn:hasExposureDuration rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf sphn:hasDuration ;
                         rdfs:domain sphn:AllergyEpisode ;
                         rdfs:range sphn:Quantity ;
                         rdfs:comment "duration of the exposure to the substance" ;
                         rdfs:label "has exposure duration" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasExposureRouteCode
sphn:hasExposureRouteCode rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf sphn:hasCode ;
                          rdfs:domain sphn:AllergyEpisode ;
                          rdfs:range [ rdf:type owl:Class ;
                                       owl:unionOf ( sphn:Code
                                                     sphn:Terminology
                                                   )
                                     ] ;
                          rdfs:comment "code, name, coding system and version describing how the individual was exposed to the substance" ;
                          rdfs:label "has exposure route code" ;
                          sphn:replaces sphn:hasAllergyEpisodeExposureRouteCode .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasFixationType
sphn:hasFixationType rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                     rdfs:domain sphn:Biosample ;
                     rdfs:range sphn:SPHNConcept ;
                     rdfs:comment "fixation or stabilization type" ;
                     rdfs:label "has fixation type" ;
                     sphn:replaces sphn:hasBiosampleFixationType .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasFractionsNumber
sphn:hasFractionsNumber rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf sphn:hasQuantity ;
                        rdfs:domain sphn:RadiotherapyProcedure ;
                        rdfs:range sphn:Quantity ;
                        rdfs:comment "number of fractions applied during an event" ;
                        rdfs:label "has fractions number" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasFrequency
sphn:hasFrequency rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sphn:hasQuantity ;
                  rdfs:domain sphn:DrugPrescription ;
                  rdfs:range sphn:Quantity ;
                  rdfs:comment "number of events per unit of time" ;
                  rdfs:label "has frequency" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasInactiveIngredient
sphn:hasInactiveIngredient rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                           rdfs:domain sphn:Drug ;
                           rdfs:range sphn:Substance ;
                           rdfs:comment "inert ingredients or excipients, and generally have no pharmacological effect" ;
                           rdfs:label "has inactive ingredient" ;
                           sphn:replaces sphn:hasDrugInactiveIngredientSubstance .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasIndicationToStart
sphn:hasIndicationToStart rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                          rdfs:domain sphn:DrugPrescription ;
                          rdfs:range sphn:SPHNConcept ;
                          rdfs:comment "indication to start the drug treatment; can be a specific FOPH diagnosis, or a generic reason" ;
                          rdfs:label "has indication to start" ;
                          sphn:replaces sphn:hasDrugPrescriptionIndicationToStart .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasInstrument
sphn:hasInstrument rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                   rdfs:domain sphn:LabTest ;
                   rdfs:range sphn:LabAnalyzer ;
                   rdfs:comment "instrument used to assess medical laboratory samples" ;
                   rdfs:label "has instrument" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasIntent
sphn:hasIntent rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf sphn:SPHNAttributeObject ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( sphn:DrugPrescription
                                           sphn:Procedure
                                         )
                           ] ;
               rdfs:range sphn:Intent ;
               rdfs:comment "intention for the concept" ;
               rdfs:label "has intent" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasLabTest
sphn:hasLabTest rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                rdfs:domain sphn:LabResult ;
                rdfs:range sphn:LabTest ;
                rdfs:comment "lab test information including information elements provided by LOINC, instrument and test kit" ;
                rdfs:label "has lab test" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasLaterality
sphn:hasLaterality rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                   rdfs:domain sphn:BodySite ;
                   rdfs:range sphn:Laterality ;
                   rdfs:comment "localization with respect to the side of the body" ;
                   rdfs:label "has laterality" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasLocation
sphn:hasLocation rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sphn:SPHNAttributeObject ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( sphn:AdministrativeCase
                                             sphn:HealthcareEncounter
                                           )
                             ] ;
                 rdfs:range sphn:Location ;
                 rdfs:comment "statement about a physical location" ;
                 rdfs:label "has location" .


###  https://biomedit.ch/rdf/sphn-ontology/sphn#hasLowerLimit
sphn:hasLowerLimit rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sphn:hasQuantity ;
                   rdfs:domain sphn:ReferenceRange ;
                   rdfs:range sphn:Quantity ;
